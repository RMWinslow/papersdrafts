\end_layout

\begin_layout Plain Layout
-
\end_layout

\end_inset


\end_layout

\begin_layout Standard
The derivatives of numerator and denominator are
\end_layout

\begin_layout Standard
(Note with work shown 
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
\frac{\partial}{\partial N_{j}}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right] & =\frac{\partial}{\partial N_{j}}\left[\sum_{i\neq j}A_{i}N_{i}e^{(U-1)TN_{i}}\right]+\frac{\partial}{\partial N_{j}}\left[A_{j}N_{j}e^{(U-1)TN_{j}}\right]\\
 & =...
\end{align*}

\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial N_{j}}\left[A_{j}N_{j}e^{(U-1)TN_{j}}\right]=A_{j}e^{(U-1)TN_{j}}\cdot\left[1+\frac{\partial U}{\partial N_{j}}TN_{j}^{2}+TUN_{j}-TN_{j}\right]
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial N_{j}}\left[\sum_{i\neq j}A_{i}N_{i}e^{(U-1)TN_{i}}\right]=\sum_{i\neq j}\left[A_{i}N_{i}^{2}Te^{(U-1)TN_{i}}\frac{\partial U}{\partial N_{j}}\right]
\]

\end_inset


\end_layout

\end_inset

)
\begin_inset Formula 
\begin{align*}
\frac{\partial}{\partial N_{j}}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right] & =\frac{\partial}{\partial N_{j}}\left[\sum_{i\neq j}A_{i}N_{i}e^{(U-1)TN_{i}}\right]+\frac{\partial}{\partial N_{j}}\left[A_{j}N_{j}e^{(U-1)TN_{j}}\right]\\
 & =\sum_{i\neq j}\left[A_{i}N_{i}^{2}Te^{(U-1)TN_{i}}\frac{\partial U}{\partial N_{j}}\right]+A_{j}e^{(U-1)TN_{j}}\cdot\left[1+\frac{\partial U}{\partial N_{j}}TN_{j}^{2}+TUN_{j}-TN_{j}\right]\\
 & =\sum_{i}\left[A_{i}N_{i}^{2}Te^{(U-1)TN_{i}}\frac{\partial U}{\partial N_{j}}\right]+A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
And 
\begin_inset Formula 
\[
\frac{\partial}{\partial N_{j}}\left[\sum_{i}A_{i}N_{i}\right]=A_{j}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial U}{\partial N_{j}} & =\frac{\left(\sum_{i}\left[A_{i}N_{i}^{2}Te^{(U-1)TN_{i}}\frac{\partial U}{\partial N_{j}}\right]+A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\right)\left[\sum_{i}A_{i}N_{i}\right]-A_{j}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}\\
 & =\frac{\sum_{i}\left[A_{i}N_{i}^{2}Te^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]}\frac{\partial U}{\partial N_{j}}+\frac{\left(A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\right)}{\left[\sum_{i}A_{i}N_{i}\right]}-\frac{A_{j}}{\left[\sum_{i}A_{i}N_{i}\right]}U\\
 & =\frac{\sum_{i}\left[A_{i}N_{i}^{2}Te^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]}\frac{\partial U}{\partial N_{j}}+A_{j}\frac{\left(e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-U\right)}{\left[\sum_{i}A_{i}N_{i}\right]}
\end{align*}

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial N_{j}}\cdot\left[1-\frac{\sum_{i}\left[A_{i}N_{i}^{2}Te^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]}\right]=A_{j}\frac{\left(e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-U\right)}{\left[\sum_{i}A_{i}N_{i}\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial U}{\partial N_{j}}=\frac{\left[A_{j}\frac{\left(e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-U\right)}{\left[\sum_{i}A_{i}N_{i}\right]}\right]}{\left[1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
As discussed below, the denominator 
\begin_inset Formula $1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}>0$
\end_inset

 is positive whenever an epidemic is possible (
\begin_inset Formula $T>T_{c}$
\end_inset

).
\end_layout

\begin_layout Standard
As such, the entire derivative is positive iff the term 
\begin_inset Formula $e^{(U-1)TN_{j}}\cdot\left[1-(1-U)TN_{j}\right]-U>0$
\end_inset

 
\end_layout

\begin_layout Standard
Clearly, it's negative if 
\begin_inset Formula $N_{j}>\frac{1}{(1-U)T}$
\end_inset

, meaning 
\series bold
a fatalistic person always decreases 
\begin_inset Formula $U$
\end_inset

, increases 
\begin_inset Formula $\Psi$
\end_inset

 when they increase connections
\series default
.
 But what about when it's lower? What else can we say about this threshold?
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
e^{(U-1)TN_{j}}\cdot\left[1-(1-U)TN_{j}\right]>U=\frac{\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset

Note about rearranging this to seperate the 
\begin_inset Formula $jth$
\end_inset

 term.
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\[
U=\frac{\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}=e^{(U-1)TN_{j}}\cdot\frac{\sum_{i}A_{i}N_{i}\frac{e^{(U-1)TN_{i}}}{e^{(U-1)TN_{j}}}}{\sum_{i}A_{i}N_{i}}=e^{(U-1)TN_{j}}\frac{\sum_{i}A_{i}N_{i}e^{(U-1)T\cdot\left[N_{i}-N_{j}\right]}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\begin_inset Formula 
\[
e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-U=...
\]

\end_inset


\begin_inset Formula 
\begin{align*}
 & =e^{(U-1)TN_{j}}\cdot\left[1-(1-U)TN_{j}-\frac{\sum_{i}A_{i}N_{i}e^{(U-1)T\cdot\left[N_{i}-N_{j}\right]}}{\sum_{i}A_{i}N_{i}}\right]\\
 & =e^{(U-1)TN_{j}}\cdot\left[\frac{\sum_{i}A_{i}N_{i}}{\sum_{i}A_{i}N_{i}}-N_{j}T\frac{\sum_{i}A_{i}N_{i}\left(1-e^{(U-1)TN_{i}}\right)}{\sum_{i}A_{i}N_{i}}-\frac{\sum_{i}A_{i}N_{i}e^{(U-1)T\cdot\left[N_{i}-N_{j}\right]}}{\sum_{i}A_{i}N_{i}}\right]\\
 & =\frac{e^{(U-1)TN_{j}}}{\sum_{i}A_{i}N_{i}}\cdot\left[\sum_{i}A_{i}N_{i}-N_{j}T\sum_{i}A_{i}N_{i}\left(1-e^{(U-1)TN_{i}}\right)-\sum_{i}A_{i}N_{i}e^{(U-1)T\cdot\left[N_{i}-N_{j}\right]}\right]\\
 & =\frac{e^{(U-1)TN_{j}}}{\sum_{i}A_{i}N_{i}}\cdot\left[\sum_{i}A_{i}N_{i}\left(1-e^{(U-1)T\cdot\left[N_{i}-N_{j}\right]}\right)-N_{j}T\sum_{i}A_{i}N_{i}\left(1-e^{(U-1)TN_{i}}\right)\right]\\
 & =\frac{e^{(U-1)TN_{j}}}{\sum_{i}A_{i}N_{i}}\cdot\left[\sum_{i}A_{i}N_{i}\left(1-e^{(U-1)T\cdot\left[N_{i}-N_{j}\right]}-N_{j}T\left(1-e^{(U-1)TN_{i}}\right)\right)\right]\\
 & =\frac{e^{(U-1)TN_{j}}}{\sum_{i}A_{i}N_{i}}\cdot\left[\sum_{i\neq j}A_{i}N_{i}\left(1-e^{(U-1)T\cdot\left[N_{i}-N_{j}\right]}-N_{j}T\left(1-e^{(U-1)TN_{i}}\right)\right)-N_{j}TA_{j}N_{j}\left(1-e^{(U-1)TN_{j}}\right)\right]\\
 & =\frac{e^{(U-1)TN_{j}}}{\sum_{i}A_{i}N_{i}}\cdot\left[\sum_{i\neq j}A_{i}N_{i}\left(1-\frac{e^{(U-1)TN_{i}}}{e^{(U-1)TN_{j}}}-N_{j}T+N_{j}Te^{(U-1)TN_{i}}\right)-N_{j}TA_{j}N_{j}p(N_{j})\right]\\
 & =\frac{e^{(U-1)TN_{j}}}{\sum_{i}A_{i}N_{i}}\cdot\left[\sum_{i\neq j}A_{i}N_{i}\left(1-N_{j}T+\left[N_{j}T-e^{(1-U)TN_{j}}\right]\cdot e^{(U-1)TN_{i}}\right)-N_{j}TA_{j}N_{j}p(N_{j})\right]
\end{align*}

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
This isn't very helpful, but at least I can say the following.
 
\end_layout

\begin_layout Standard
Because the term 
\begin_inset Formula $N_{j}TA_{j}N_{j}\left(1-e^{(U-1)TN_{j}}\right)=N_{j}TA_{j}N_{j}p(N_{j})>0$
\end_inset

, 
\series bold
if there is only one type of person
\series default
, it must be that 
\begin_inset Formula $e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-U<0$
\end_inset

 and thus that 
\begin_inset Formula $\frac{\partial U}{\partial N_{j}}<0$
\end_inset

, meaning that 
\series bold
an increase in the connections of that singular type will always increase
 the risk per connection whenever epidemic is possible.
\end_layout

\begin_layout Standard
Okay, what about solving for 
\begin_inset Formula $U$
\end_inset

 or 
\begin_inset Formula $\Psi$
\end_inset

?
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
e^{(U-1)TN_{j}}\cdot\left[1-(1-U)TN_{j}\right]-U & =e^{(U-1)TN_{j}}-TN_{j}e^{(U-1)TN_{j}}+UTN_{j}e^{(U-1)TN_{j}}-U\\
 & =e^{(U-1)TN_{j}}\left[1-TN_{j}\right]+U\cdot\left[TN_{j}e^{(U-1)TN_{j}}-1\right]
\end{align*}

\end_inset


\begin_inset Formula 
\[
e^{(U-1)TN_{j}}\cdot\left[1-(1-U)TN_{j}\right]-U>0
\]

\end_inset


\begin_inset Formula 
\[
e^{(U-1)TN_{j}}\left[1-TN_{j}\right]+U\cdot\left[TN_{j}e^{(U-1)TN_{j}}-1\right]>0
\]

\end_inset


\begin_inset Formula 
\[
U\cdot\left[TN_{j}e^{(U-1)TN_{j}}-1\right]>e^{(U-1)TN_{j}}\left[TN_{j}-1\right]
\]

\end_inset


\begin_inset Formula 
\[
U\cdot\left[1-TN_{j}e^{(U-1)TN_{j}}\right]<e^{(U-1)TN_{j}}\left[1-TN_{j}\right]
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
Now assume that 
\begin_inset Formula $N_{j}<\frac{1}{\Psi}$
\end_inset

.
 And further
\series bold
 assume that 
\begin_inset Formula $N_{j}<\frac{1}{T}$
\end_inset

 
\series default
as well such that 
\begin_inset Formula $TN_{j}<1$
\end_inset

.
 If this is the case, then (COME BACK TO CLEAN THIS UP)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left[1-TN_{j}e^{(U-1)TN_{j}}\right]>\left[1-e^{(U-1)TN_{j}}\right]=p(N_{j})>0
\]

\end_inset


\end_layout

\begin_layout Standard
So 
\begin_inset Formula $e^{(U-1)TN_{j}}\cdot\left[1-(1-U)TN_{j}\right]-U>0$
\end_inset

 and 
\begin_inset Formula $\frac{\partial U}{\partial N_{j}}>0$
\end_inset

 iff 
\begin_inset Formula 
\[
U<\frac{e^{(U-1)TN_{j}}\left[1-TN_{j}\right]}{\left[1-TN_{j}e^{(U-1)TN_{j}}\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
Rewrite in terms of 
\begin_inset Formula $\Psi=(1-U)T$
\end_inset

 to get 
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
that we have the counterintutive results when 
\begin_inset Formula $N_{j}<\frac{1}{\Psi}$
\end_inset

 and
\begin_inset Formula 
\[
\Psi>T\left[1-\frac{e^{-\Psi N_{j}}\left[1-TN_{j}\right]}{\left[1-TN_{j}e^{-\Psi N_{j}}\right]}\right]=T\frac{1-TN_{j}e^{-\Psi N_{j}}-e^{-\Psi N_{j}}\left[1-TN_{j}\right]}{\left[1-TN_{j}e^{-\Psi N_{j}}\right]}
\]

\end_inset


\begin_inset Formula 
\[
\Psi>T\frac{1-e^{-\Psi N_{j}}}{\left[1-TN_{j}e^{-\Psi N_{j}}\right]}=T\frac{p(N_{j})}{\left[1-TN_{j}e^{-\Psi N_{j}}\right]}
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
Take the derivative of the RHS to get 
\begin_inset Formula $\frac{e^{(U-1)TN_{j}}\left[1-TN_{j}\right]TN_{j}}{\left[1-TN_{j}e^{(U-1)TN_{j}}\right]^{2}}$
\end_inset

 and evaluate at 
\begin_inset Formula $1$
\end_inset

 to get 
\begin_inset Formula $\frac{\left[1-TN_{j}\right]TN_{j}}{\left[1-TN_{j}\right]^{2}}=\frac{TN_{j}}{1-TN_{j}}$
\end_inset

 If this is 
\begin_inset Formula $>1$
\end_inset

 , which happens when 
\begin_inset Formula $N_{j}<\frac{1}{2T}$
\end_inset

 then there it is ensured that 
\begin_inset Formula $U<\frac{e^{(U-1)TN_{j}}\left[1-TN_{j}\right]}{\left[1-TN_{j}e^{(U-1)TN_{j}}\right]}$
\end_inset

 in the epidemic solution for 
\begin_inset Formula $U$
\end_inset

.
 (Note that if 
\begin_inset Formula $j$
\end_inset

 is the only type, this would put 
\begin_inset Formula $T<\frac{1}{2}\frac{1}{N}<T_{c}$
\end_inset

 , and the epidemic solution wouldn't exist.
 
\series bold
Thus if epidemic is possible, and 
\begin_inset Formula $N_{j}<\frac{1}{2T}$
\end_inset

, then 
\begin_inset Formula $\frac{\partial U}{\partial N_{j}}>0$
\end_inset

 , meaning that an increase in connections among type 
\begin_inset Formula $j$
\end_inset

 reduces the edge risk
\series default
.
 
\end_layout

\begin_layout Standard
But the more general statment is that this happens when 
\begin_inset Formula $U<\frac{e^{(U-1)TN_{j}}\left[1-TN_{j}\right]}{\left[1-TN_{j}e^{(U-1)TN_{j}}\right]}$
\end_inset

.
 When does 
\begin_inset Formula $U=\frac{e^{(U-1)TN_{j}}\left[1-TN_{j}\right]}{\left[1-TN_{j}e^{(U-1)TN_{j}}\right]}$
\end_inset

? That's the important threshold.
 And plugging in the definition for U we get
\begin_inset Formula 
\[
\frac{e^{(U-1)TN_{j}}\left[1-TN_{j}\right]}{\left[1-TN_{j}e^{(U-1)TN_{j}}\right]}=\frac{\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\end_layout

\begin_layout Standard
Or in terms of 
\begin_inset Formula $\Psi$
\end_inset


\begin_inset Formula 
\[
T\frac{1-e^{-\Psi N_{j}}}{\left[1-TN_{j}e^{-\Psi N_{j}}\right]}=T-T\frac{\sum_{i}A_{i}N_{i}e^{-\Psi N_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{1-e^{-\Psi N_{j}}}{\left[1-TN_{j}e^{-\Psi N_{j}}\right]}=1-\frac{\sum_{i}A_{i}N_{i}e^{-\Psi N_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\end_layout

\begin_layout Standard
Won't work if 
\begin_inset Formula $N_{j}\geq\frac{1}{T}$
\end_inset

.
 So the threshold is below that at least.
 (Or is that just a consequence of my assumptions above?)
\end_layout

\begin_layout Subsection*
Partial derivative wrt T:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial U}{\partial T}=\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\left[\frac{\partial U}{\partial T}T+U-1\right]}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial T}=\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\left[U-1\right]}{\sum_{i}A_{i}N_{i}}+\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\frac{\partial U}{\partial T}T}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial T}\cdot\left[1-\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}T}{\sum_{i}A_{i}N_{i}}\right]=\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\left[U-1\right]}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial T}=\frac{\left[\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\left[U-1\right]}{\sum_{i}A_{i}N_{i}}\right]}{\left[1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
As 
\begin_inset Formula $T\to T_{c}$
\end_inset

 from the right, 
\begin_inset Formula $\lim\frac{\partial U}{\partial T}=\frac{-0\cdot\frac{1}{T_{c}}}{1-T_{c}\frac{1}{T_{c}}}=\frac{0}{0}.$
\end_inset

Intdterminate.
 But based on the note below, I'm guessing it converges to something finite
 but negative.
 
\end_layout

\end_inset


\end_layout

\begin_layout Standard
So already we can observe that (for 
\begin_inset Formula $\sum_{i}A_{i}N_{i}\neq0$
\end_inset

), the numerator is clearly non-positive.
 And it's negative unless 
\begin_inset Formula $U=1$
\end_inset

 , meaning outbreak extinction is ensured.
 And when pandemic can occur, the denominator will always be positive.
\end_layout

\begin_layout Standard
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
Furthermore, if 
\begin_inset Formula $N_{i}\geq1$
\end_inset

 
\begin_inset Formula $\forall i$
\end_inset

, then 
\begin_inset Formula $\left[1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}\right]\geq\left[1-T\frac{\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}\right]=1-TU\geq0$
\end_inset

, meaning that an increase in 
\begin_inset Formula $T$
\end_inset

, holding 
\begin_inset Formula ${N}$
\end_inset

 fixed, will decrease 
\begin_inset Formula $U$
\end_inset

, meaning that the edge-weighted prevalence of the disease becomes more
 likely.
 
\end_layout

\begin_layout Plain Layout
Furthermore, note that 
\begin_inset Formula $e^{(U-1)TN_{i}}\in[0,1]$
\end_inset

, and so 
\begin_inset Formula $T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}\leq T\frac{\sum_{i}A_{i}N_{i}^{2}}{\sum_{i}A_{i}N_{i}}=R_{0}$
\end_inset

.
 And thus the denominator 
\begin_inset Formula $\left[1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}\right]\geq\left[1-R_{0}\right]$
\end_inset

.
 But wait this isn't super useful.
 That's Negative whenever an outbreak is possible, and I want to show that
 (when outbreak is possible, meaning 
\begin_inset Formula $U\neq1$
\end_inset

) the denominator is positive.
\end_layout

\begin_layout Plain Layout
Mrrm.
 Rearranging it a bit, it becomes
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\partial U}{\partial T}=\frac{\left[\left[U-1\right]\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}-T\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\right]}=\frac{\left[\left[U-1\right]E[(1-p(N))N^{2}]\right]}{\left[E[N]-TE[(1-p(N))N^{2}]\right]}
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard

\series bold
THEOREM:
\series default
 Holding 
\begin_inset Formula $\left\{ N_{i}\right\} ,\left\{ A_{i}\right\} ,T$
\end_inset

 constant, if 
\begin_inset Formula $T>T_{c}\equiv\frac{\sum_{i}A_{i}N_{i}}{\sum_{i}A_{i}N_{i}^{2}}$
\end_inset

, then 
\begin_inset Formula $\frac{\partial U}{\partial T}<0$
\end_inset

 at the pandemic solution to 
\begin_inset Formula $U$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
PROOF:
\series default
 If 
\begin_inset Formula $T>T_{c}$
\end_inset

, then a solution for 
\begin_inset Formula $U$
\end_inset

 can be found 
\begin_inset Formula $\in(0,1)$
\end_inset

.
 Call it 
\begin_inset Formula $U^{*}$
\end_inset

.
\end_layout

\begin_layout Standard
Note 
\begin_inset Formula $\left[U^{*}-1\right]\cdot\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U^{*}-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}<0$
\end_inset


\end_layout

\begin_layout Standard
Additionally, note that 
\begin_inset Formula $1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}>0$
\end_inset

:
\end_layout

\begin_layout Itemize
The only solutions to the following equation are (by definition) 
\begin_inset Formula $x=U^{*}$
\end_inset

 and 
\begin_inset Formula $x=1$
\end_inset

.
\begin_inset Formula 
\[
0=x-\frac{\sum_{i}A_{i}N_{i}e^{(x-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\end_layout

\begin_layout Itemize
At any point 
\begin_inset Formula $x\in(U^{*},1)$
\end_inset

, this equation will be positive.
 , and for any 
\begin_inset Formula $x\notin[U^{*},1]$
\end_inset

, this equation will be negative.
 (second term is concave up).
\end_layout

\begin_layout Itemize
Taking the derivative of the RHS yields 
\begin_inset Formula $\frac{\partial}{\partial x}\left[x-\frac{\sum_{i}A_{i}N_{i}e^{(x-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}\right]=1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(x-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}$
\end_inset


\end_layout

\begin_layout Itemize
Set this derivative equal to zero to find the unique solution 
\begin_inset Formula $\underline{x}$
\end_inset

, which is the unique argmax for 
\begin_inset Formula $x-\frac{\sum_{i}A_{i}N_{i}e^{(x-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}$
\end_inset

.
 It must be that 
\begin_inset Formula $\underline{x}\in(U^{*},1)$
\end_inset

.
\end_layout

\begin_layout Itemize
Therefore, 
\begin_inset Formula $e^{(U-1)TN_{i}}<e^{(\underline{x}-1)TN_{i}}$
\end_inset

 for any 
\begin_inset Formula $N_{i}>0$
\end_inset

 and 
\begin_inset Formula $T>0$
\end_inset

, and so 
\begin_inset Formula 
\[
1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}>1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(\underline{x}-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Note
status collapsed

\begin_layout Subsubsection*
With a single type,
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\partial U}{\partial T}=\frac{\left[\left[U-1\right]N^{2}e^{(U-1)TN}\right]}{\left[N-TN^{2}e^{(U-1)TN}\right]}=\frac{\left[\left[U-1\right]Ne^{(U-1)TN}\right]}{\left[1-TNe^{(U-1)TN}\right]}
\]

\end_inset


\end_layout

\begin_layout Plain Layout
If 
\begin_inset Formula $N\leq1$
\end_inset

, then 
\begin_inset Formula $R_{0}=TN\leq1$
\end_inset

 and thus 
\begin_inset Formula $U=1$
\end_inset

, meaning 
\begin_inset Formula $\frac{\partial U}{\partial T}=0$
\end_inset

.
\end_layout

\begin_layout Plain Layout
If 
\begin_inset Formula $N>1$
\end_inset

, then as discussed above, either 
\begin_inset Formula $U=1$
\end_inset

 or 
\begin_inset Formula $\frac{\partial U}{\partial T}<0$
\end_inset

.
\end_layout

\begin_layout Plain Layout
Therefore in the single type case, 
\begin_inset Formula $\frac{\partial U}{\partial T}\leq0$
\end_inset


\end_layout

\begin_layout Plain Layout
Unimportant note about limit of 
\begin_inset Formula $\frac{\partial U}{\partial T}$
\end_inset

 as 
\begin_inset Formula $R_{0}\to1$
\end_inset

 : 
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
Actually, playing around in Desmos, I'm not sure this is right.
 The graph I plugged in makes it look like it goes to -2.
 But this math says it goes to zero.
 Confused.
\end_layout

\begin_layout Plain Layout
(Sidenote: In the case with a single type, 
\begin_inset Formula $\lim_{N\to\frac{1}{T}}\frac{\partial U}{\partial T}$
\end_inset

 is 
\begin_inset Formula $0$
\end_inset

 from the left, and 
\begin_inset Formula $-1$
\end_inset

 from the right.
 (See notes [7-09 C]).
\end_layout

\begin_layout Plain Layout
And for 
\begin_inset Formula $\lim_{T\to\frac{1}{N}}\frac{\partial U}{\partial T}$
\end_inset

, it's again 
\begin_inset Formula $0$
\end_inset

 from the left, but from the right, we need to apply Lhopital
\begin_inset Formula 
\begin{align*}
\frac{\partial}{\partial T}\left[\left[U-1\right]Ne^{(U-1)TN}\right] & =N\left[\left[U-1\right]\frac{\partial}{\partial T}e^{(U-1)TN}+e^{(U-1)TN}\frac{\partial}{\partial T}\left[U-1\right]\right]\\
 & =N\left[\left[U-1\right]N\left[T\frac{\partial U}{\partial T}+U-1\right]e^{(U-1)TN}+e^{(U-1)TN}\frac{\partial U}{\partial T}\right]\\
 & =Ne^{(U-1)TN}\left[\left[U-1\right]^{2}N+\left[U-1\right]NT\frac{\partial U}{\partial T}+\frac{\partial U}{\partial T}\right]
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial}{\partial T}\left[1-TNe^{(U-1)TN}\right] & =-N\left[T\frac{\partial}{\partial T}e^{(U-1)TN}+e^{(U-1)TN}\frac{\partial}{\partial T}T\right]\\
 & =-N\left[TN\left[T\frac{\partial U}{\partial T}+U-1\right]e^{(U-1)TN}+e^{(U-1)TN}\right]\\
 & =-Ne^{(U-1)TN}\left[TNT\frac{\partial U}{\partial T}+TN\left[U-1\right]+1\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\frac{\partial}{\partial T}\left[\left[U-1\right]Ne^{(U-1)TN}\right]}{\frac{\partial}{\partial T}\left[1-TNe^{(U-1)TN}\right]}=\frac{\left[U-1\right]^{2}N+\left[U-1\right]NT\frac{\partial U}{\partial T}+\frac{\partial U}{\partial T}}{\left[TNT\frac{\partial U}{\partial T}+TN\left[U-1\right]+1\right]}
\]

\end_inset


\end_layout

\begin_layout Plain Layout
And so coming from the right, (letting 
\begin_inset Formula $U_{lim}^{\prime}$
\end_inset

 be shorthand for 
\begin_inset Formula $\lim_{T\to_{+}\frac{1}{N}}\frac{\partial U}{\partial T}$
\end_inset

 ), (because 
\begin_inset Formula $U\to1$
\end_inset

)
\begin_inset Formula 
\begin{align*}
U_{lim}^{\prime} & =\lim_{T\to_{+}\frac{1}{N}}\frac{\left[U-1\right]^{2}N+\left[U-1\right]NT\frac{\partial U}{\partial T}+\frac{\partial U}{\partial T}}{\left[TNT\frac{\partial U}{\partial T}+TN\left[U-1\right]+1\right]}\\
 & =\frac{U_{lim}^{\prime}}{\left[\frac{1}{N}U_{lim}^{\prime}+1\right]}
\end{align*}

\end_inset


\end_layout

\begin_layout Plain Layout
For which the only solution is 
\begin_inset Formula $U_{lim}^{\prime}=0$
\end_inset

.
 Wait, does this mean that 
\begin_inset Formula $U$
\end_inset

 is actually differentiable wrt 
\begin_inset Formula $T$
\end_inset

?
\end_layout

\begin_layout Plain Layout
Very likely I made a mistake given how bad I am at basic algebra, but nevermind.
 This isn't really important.
\end_layout

\end_inset


\end_layout

\begin_layout Subsubsection*
Example(s) with two types H L:
\end_layout

\begin_layout Plain Layout
Let 
\begin_inset Formula $A_{l}=A_{h}=\frac{1}{2}$
\end_inset

 and 
\begin_inset Formula $N_{l}=0.8$
\end_inset

, 
\begin_inset Formula $N_{h}=2.4$
\end_inset

.
 
\begin_inset Formula $E[N]=\frac{2.4+0.8}{2}=1.6$
\end_inset

.
\end_layout

\begin_layout Plain Layout
In this example, 
\begin_inset Formula $T_{c}=\frac{1}{2}$
\end_inset

 and 
\begin_inset Formula $R_{0}=2T$
\end_inset

.
 
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\partial U}{\partial T}=\frac{\left[\left[U-1\right]\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}-T\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}\right]}=\frac{\left[\left[U-1\right]\frac{0.64e^{(U-1)T\cdot0.8}+5.76e^{(U-1)T\cdot2.4}}{2}\right]}{\left[1.6-T\frac{0.64e^{(U-1)T\cdot0.8}+5.76e^{(U-1)T\cdot2.4}}{2}\right]}
\]

\end_inset


\end_layout

\begin_layout Plain Layout
Let 
\begin_inset Formula $T=0.6$
\end_inset

.
\end_layout

\begin_layout Plain Layout
Then plugging these numbers into Desmos, 
\begin_inset Formula $U\approx0.7182$
\end_inset

, and 
\begin_inset Formula $U-1\approx-0.2818$
\end_inset


\begin_inset Formula 
\[
\frac{0.64e^{(-0.2818)0.6\cdot0.8}+5.76e^{(-0.2818)0.6\cdot2.4}}{2}\approx2.19889
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial T}\approx\frac{\left[\left[-0.2818\right]\cdot2.19889\right]}{\left[1.6-0.6\cdot2.19889\right]}\approx\frac{-0.6196}{0.2807}<0
\]

\end_inset


\end_layout

\begin_layout Plain Layout
Let 
\begin_inset Formula $T=\frac{5}{6}$
\end_inset

.
 Desmos says 
\begin_inset Formula $U\approx0.3853$
\end_inset

.
 and 
\begin_inset Formula $U-1\approx-0.6147$
\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{0.64e^{(-0.6147)\frac{5}{6}0.8}+5.76e^{(-0.6147)\frac{5}{6}2.4}}{2}\approx1.05472
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial T}\approx\frac{\left[\left[-0.6147\right]\cdot1.05472\right]}{\left[1.6-\frac{5}{6}\cdot1.05472\right]}\approx\frac{-0.648}{0.721}<0
\]

\end_inset


\end_layout

\begin_layout Plain Layout
Let 
\begin_inset Formula $T=0.501$
\end_inset

.
 Desmos says 
\begin_inset Formula $U\approx0.9964$
\end_inset

.
 and 
\begin_inset Formula $U-1\approx-0.0036$
\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{0.64e^{(-0.0036)0.501\cdot0.8}+5.76e^{(-0.0036)0.501\cdot2.4}}{2}\approx3.1871
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial T}\approx\frac{\left[\left[-0.0036\right]\cdot3.1871\right]}{\left[1.6-0.501\cdot3.1871\right]}\approx\frac{-0.0115}{0.003269}<0
\]

\end_inset


\end_layout

\begin_layout Plain Layout
Doesn't seem like I can find a spot where the partial derivative is positive.
\end_layout

\end_inset


\end_layout

\begin_layout Section*
Taking partial derivatives of 
\begin_inset Formula $\Psi$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Psi=T-T\cdot U=T-T\frac{\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Partial derivative wrt T:
\end_layout

\begin_layout Standard
Even though we can't explicitly solve for 
\begin_inset Formula $\Psi,$
\end_inset

we can observe that an increase in 
\begin_inset Formula $T$
\end_inset

 will increase 
\begin_inset Formula $\Psi$
\end_inset

 when 
\begin_inset Formula $\left\{ N_{i}\right\} $
\end_inset

 are held constant (and pandemic is actually possible, meaning 
\begin_inset Formula $T>T_{c}$
\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial\Psi(T)}{\partial T}=1-U(T)-T\cdot\frac{\partial U(T)}{\partial T}>0
\]

\end_inset


\end_layout

\begin_layout Section*
Taking partial derivatives of 
\begin_inset Formula $T_{c}$
\end_inset

 and 
\begin_inset Formula $R_{0}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T_{c}=\frac{\sum_{i}A_{i}N_{i}}{\sum_{i}A_{i}N_{i}^{2}}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{\partial T_{c}}{\partial N_{j}} & =\frac{A_{j}\left[\sum_{i}A_{i}N_{i}^{2}\right]-2A_{j}N_{j}\left[\sum_{i}A_{i}N_{i}\right]}{\left(\sum_{i}A_{i}N_{i}^{2}\right)^{2}}\\
 & =A_{j}\frac{\left[\sum_{i}A_{i}N_{i}^{2}\right]-2\left[\sum_{i}A_{i}N_{i}N_{j}\right]}{\left(\sum_{i}A_{i}N_{i}^{2}\right)^{2}}\\
 & =A_{j}\frac{\left[\sum_{i\neq j}A_{i}N_{i}^{2}\right]+A_{j}N_{j}^{2}-2\left[\sum_{i\neq j}A_{i}N_{i}N_{j}\right]-2A_{j}N_{j}^{2}}{\left(\sum_{i}A_{i}N_{i}^{2}\right)^{2}}\\
 & =A_{j}\frac{\left[\sum_{i\neq j}A_{i}N_{i}^{2}\right]-2\left[\sum_{i\neq j}A_{i}N_{i}\right]N_{j}-A_{j}N_{j}^{2}}{\left(\sum_{i}A_{i}N_{i}^{2}\right)^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The sign of this is the same as the sign of 
\begin_inset Formula $\left[\sum_{i\neq j}A_{i}N_{i}^{2}\right]-2\left[\sum_{i\neq j}A_{i}N_{i}\right]N_{j}-A_{j}N_{j}^{2}$
\end_inset

 .
 
\end_layout

\begin_layout Standard
Plug these into quadratic formula to get roots at 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{-b\pm\sqrt{b^{2}-4ac}}{2a}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
N_{j\taurus} & =\frac{-\frac{2\left[\sum_{i\neq j}A_{i}N_{i}\right]}{A_{j}}+\sqrt{\left(\frac{2\left[\sum_{i\neq j}A_{i}N_{i}\right]}{A_{j}}\right)^{2}+4\frac{\left[\sum_{i\neq j}A_{i}N_{i}^{2}\right]}{A_{j}}}}{2}\\
 & =\sqrt{\left(\frac{\sum_{i\neq j}A_{i}N_{i}}{A_{j}}\right)^{2}+\frac{\sum_{i\neq j}A_{i}N_{i}^{2}}{A_{j}}}-\frac{\left[\sum_{i\neq j}A_{i}N_{i}\right]}{A_{j}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $N_{j}>N_{j\taurus}$
\end_inset

, then 
\begin_inset Formula $\frac{\partial T_{c}}{\partial N_{j}}<0$
\end_inset

 as you would expect.
\end_layout

\begin_layout Standard
But if 
\begin_inset Formula $N_{j}<N_{j\taurus}$
\end_inset

, then 
\begin_inset Formula $\frac{\partial T_{c}}{\partial N_{j}}>0$
\end_inset

, meaning that type 
\begin_inset Formula $j$
\end_inset

 increasing their connections will actually result in an 
\emph on
increase
\emph default
 in 
\begin_inset Formula $T_{c}$
\end_inset

 potentially preventing an epidemic.
 
\end_layout

\begin_layout Standard
Similarly, 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R_{0}=\frac{T}{T_{c}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial R_{0}}{\partial N_{j}}=-\frac{T\frac{\partial T_{c}}{\partial N_{j}}}{T_{c}^{2}}
\]

\end_inset

 will have the same threshold, where 
\begin_inset Formula $N_{j}<N_{j\taurus}$
\end_inset

 means that 
\begin_inset Formula $\frac{\partial R_{0}}{\partial N_{j}}<0$
\end_inset

 , slowing the spread and potentially preventing an epidemic.
\end_layout

\begin_layout Subsection*
Example with two types:
\end_layout

\begin_layout Standard
For example, in the two-type case with 
\begin_inset Formula $A_{h}=A_{l}=\frac{1}{2}$
\end_inset

, 
\begin_inset Formula 
\begin{align*}
N_{l\taurus} & =\sqrt{\left(\frac{\frac{1}{2}N_{h}}{1/2}\right)^{2}+\frac{\frac{1}{2}N_{h}^{2}}{1/2}}-\frac{\left[\frac{1}{2}N_{h}\right]}{1/2}\\
 & \sqrt{2N_{h}^{2}}-N_{h}\\
 & =N_{h}\cdot\left[\sqrt{2}-1\right]\approx N_{h}\cdot0.4142
\end{align*}

\end_inset

So for example, let 
\begin_inset Formula $N_{h}$
\end_inset

 be 
\begin_inset Formula $9$
\end_inset

 and 
\begin_inset Formula $N_{l}$
\end_inset

 be 
\begin_inset Formula $1$
\end_inset

.
 Then 
\begin_inset Formula $\frac{\partial T_{c}}{\partial N_{l}}=\frac{1}{2}\frac{\left[\frac{81}{2}\right]-2\left[\frac{9}{2}\right]-\frac{1}{2}}{\left(\frac{82}{2}\right)^{2}}=\frac{81-18-1}{82^{2}}=\frac{62}{82^{2}}\approx0.00922>0$
\end_inset

.
\end_layout

\begin_layout Standard
And 
\begin_inset Formula $T_{c}=\frac{5}{41},$
\end_inset

so 
\begin_inset Formula $\frac{\partial R_{0}}{\partial N_{l}}=-\frac{T\frac{\partial T_{c}}{\partial N_{l}}}{T_{c}^{2}}=-T\frac{\left(\frac{62}{82^{2}}\right)}{T_{c}^{2}}=<0$
\end_inset


\end_layout

\begin_layout Standard
So type 
\begin_inset Formula $l$
\end_inset

 increasing their connections will result in greater 
\begin_inset Formula $R_{0}$
\end_inset

 and lower 
\begin_inset Formula $T_{c}$
\end_inset

.
 But how will it affect 
\begin_inset Formula $U$
\end_inset

 and how will it affect 
\begin_inset Formula $R_{\infty}?$
\end_inset

 
\end_layout

\begin_layout Standard
We need 
\begin_inset Formula $N_{l}<\frac{1}{\Psi}=\frac{1}{(1-U)T}$
\end_inset

.
 With 
\begin_inset Formula $T=0.2,$
\end_inset

 
\begin_inset Formula $U\approx0.3871$
\end_inset

, we do indeed get 
\begin_inset Formula $N_{l}<\frac{1}{(1-U)T}\approx\frac{1}{0.12258}\approx8.16$
\end_inset

.
 In fact, neither person here is fatalistic.
 
\begin_inset Formula $N_{l}e^{(U-1)TN_{l}}\approx0.8846351313$
\end_inset

 and 
\begin_inset Formula $N_{i}e^{(U-1)TN_{i}}\approx2.9862086637$
\end_inset

, so 
\begin_inset Formula $\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\approx1.9354218975$
\end_inset

.
\end_layout

\begin_layout Standard
This is actually using the incorrect formula: 
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
\frac{\partial U}{\partial N_{l}} & =\frac{\left[\frac{A_{l}e^{(U-1)TN_{l}}\cdot\left[1+TUN_{l}-TN_{l}\right]\left[\sum_{i}A_{i}N_{i}\right]-A_{l}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}\right]}{\left[1-\frac{A_{j}e^{(U-1)TN_{j}}TN_{j}^{2}}{\left[\sum_{i}A_{i}N_{i}\right]}\right]}\\
 & \approx\frac{\left[\frac{0.5\cdot0.8846351313\cdot0.87742\cdot5-0.5\cdot1.9354218975}{\left[5\right]^{2}}\right]}{\left[1-0.5\frac{0.1769270263}{\left[5\right]}\right]}\\
 & \approx\frac{\left[\frac{0.972780443513115}{\left[5\right]^{2}}\right]}{\left[0.98230729737\right]}\approx0.0396>0
\end{align*}

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
The above example was incorrect, but just using Desmos, I was able to show
 that indeed, with these parameters, type 
\begin_inset Formula $l$
\end_inset

 at the margins can increase 
\begin_inset Formula $U$
\end_inset

 and thus decrease 
\begin_inset Formula $\Psi$
\end_inset

, the disease risk.
 
\end_layout

\begin_layout Standard
It looks like holding 
\begin_inset Formula $N_{h}$
\end_inset

 constant at 
\begin_inset Formula $9$
\end_inset

, an increase in type 
\begin_inset Formula $l$
\end_inset

's contacts reduces disease until around 
\begin_inset Formula $N_{l}\approx3.25$
\end_inset

, at which point, 
\begin_inset Formula $U$
\end_inset

 peaks at 
\begin_inset Formula $\approx.4727$
\end_inset

.
 Confusingly, this doesn't correspond to the fatalism threshold nor to the
 threshold 
\begin_inset Formula $N_{h}\cdot0.4142\approx3.72$
\end_inset

 at which 
\begin_inset Formula $R_{0}$
\end_inset

 starts increasing.
 So that means that as type 
\begin_inset Formula $l$
\end_inset

 increases connection, first we hit a point where they start making the
 disease more common, and then only after that do we hit the point where
 the make the disease spread more??!! Confusing.
\end_layout

