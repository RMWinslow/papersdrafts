\begin_body

\begin_layout Standard
\begin_inset Formula 
\[
\Psi=T-T\frac{\sum_{i}A_{i}N_{i}e^{-\Psi N_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\end_layout

\begin_layout Standard
Keeping in mind that the 
\begin_inset Formula $\Psi$
\end_inset

 on the RHS needs to be treated as a function of 
\begin_inset Formula $N_{i}$
\end_inset

, let's find the derivative in the case where there are two types of people:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Psi=T-T\frac{A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}}{A_{h}N_{h}+A_{l}N_{l}}
\]

\end_inset


\end_layout

\begin_layout Standard
The derivatives of the numerator and denominator are 
\begin_inset Formula 
\begin{align*}
\frac{\partial}{\partial N_{l}}[A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}] & =-A_{l}e^{-\Psi N_{l}}\cdot\left[N_{l}^{2}\frac{\partial\Psi}{\partial N_{l}}+N_{l}\Psi-1\right]\\
 & =-A_{l}e^{-\Psi N_{l}}N_{l}^{2}\frac{\partial\Psi}{\partial N_{l}}-A_{l}e^{-\Psi N_{l}}N_{l}\Psi+A_{l}e^{-\Psi N_{l}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial}{\partial N_{l}}[A_{h}N_{h}+A_{l}N_{l}]=A_{l}
\]

\end_inset


\end_layout

\begin_layout Standard
And so applying the chain rule we get 
\begin_inset Formula 
\begin{align*}
\frac{\partial\Psi}{\partial N_{l}} & =-T\frac{\left[-A_{l}e^{-\Psi N_{l}}N_{l}^{2}\frac{\partial\Psi}{\partial N_{l}}-A_{l}e^{-\Psi N_{l}}N_{l}\Psi+A_{l}e^{-\Psi N_{l}}\right]\left[A_{h}N_{h}+A_{l}N_{l}\right]-A_{l}\left[A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]^{2}}\\
 & =-T\frac{\left[-A_{l}e^{-\Psi N_{l}}N_{l}^{2}\frac{\partial\Psi}{\partial N_{l}}-A_{l}e^{-\Psi N_{l}}N_{l}\Psi+A_{l}e^{-\Psi N_{l}}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]}+T\frac{A_{l}\left[A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]^{2}}\\
 & =T\frac{\left[A_{l}e^{-\Psi N_{l}}N_{l}^{2}\frac{\partial\Psi}{\partial N_{l}}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]}+T\frac{\left[A_{l}e^{-\Psi N_{l}}N_{l}\Psi-A_{l}e^{-\Psi N_{l}}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]}+T\frac{A_{l}\left[A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]^{2}}\\
 & =\frac{\left[TA_{l}e^{-\Psi N_{l}}N_{l}^{2}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]}\frac{\partial\Psi}{\partial N_{l}}+T\frac{A_{l}e^{-\Psi N_{l}}\left[N_{l}\Psi-1\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]}+T\frac{A_{l}\left[A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]}{\left[A_{h}N_{h}+A_{l}N_{l}\right]^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Or writing it more concisely with 
\begin_inset Formula $\mu\equiv A_{h}N_{h}+A_{l}N_{l}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\frac{\partial\Psi}{\partial N_{l}} & =\frac{\left[TA_{l}e^{-\Psi N_{l}}N_{l}^{2}\right]}{\mu}\frac{\partial\Psi}{\partial N_{l}}+T\frac{A_{l}e^{-\Psi N_{l}}\left[N_{l}\Psi-1\right]}{\mu}+T\frac{A_{l}\left[A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]}{\mu^{2}}\\
 & =\frac{\left[TA_{l}e^{-\Psi N_{l}}N_{l}^{2}\right]}{\mu}\frac{\partial\Psi}{\partial N_{l}}+T\frac{A_{l}e^{-\Psi N_{l}}\left[N_{l}\Psi-1\right]\mu}{\mu^{2}}+T\frac{A_{l}\left[A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]}{\mu^{2}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\left[1-\frac{\left[TA_{l}e^{-\Psi N_{l}}N_{l}^{2}\right]}{\mu}\right]\cdot\frac{\partial\Psi}{\partial N_{l}} & =\frac{TA_{l}}{\mu^{2}}\cdot\left[e^{-\Psi N_{l}}\left[N_{l}\Psi-1\right]\mu+A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]\\
\left[\mu-\left[TA_{l}e^{-\Psi N_{l}}N_{l}^{2}\right]\right]\cdot\frac{\partial\Psi}{\partial N_{l}} & =\frac{TA_{l}}{\mu}\cdot\left[e^{-\Psi N_{l}}\left[N_{l}\Psi-1\right]\mu+A_{h}N_{h}e^{-\Psi N_{h}}+A_{l}N_{l}e^{-\Psi N_{l}}\right]\\
 & =TA_{l}\cdot\left[e^{-\Psi N_{l}}\left[N_{l}\Psi-1\right]+\frac{A_{h}N_{h}}{\mu}e^{-\Psi N_{h}}+\frac{A_{l}N_{l}}{\mu}e^{-\Psi N_{l}}\right]\\
 & =TA_{l}\cdot\left[e^{-\Psi N_{l}}\left[\frac{A_{l}N_{l}}{\mu}+N_{l}\Psi-1\right]+\frac{A_{h}N_{h}}{\mu}e^{-\Psi N_{h}}\right]
\end{align*}

\end_inset


\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N_{l}}=\frac{TA_{l}\cdot\left[e^{-\Psi N_{l}}\left[\frac{A_{l}N_{l}}{\mu}+N_{l}\Psi-1\right]+\frac{A_{h}N_{h}}{\mu}e^{-\Psi N_{h}}\right]}{\mu-\left[TA_{l}e^{-\Psi N_{l}}N_{l}^{2}\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
This seems inconsistent with the single-type derivative I calculated.
\end_layout

\begin_layout Standard
Let's double check.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Psi=T-Te^{-\Psi N}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N}=Te^{-\Psi N}\cdot\left[\frac{\partial\Psi}{\partial N}\cdot N+\Psi\right]
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N}=TNe^{-\Psi N}\frac{\partial\Psi}{\partial N}+T\Psi e^{-\Psi N}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N}\cdot\left[1-TNe^{-\Psi N}\right]=T\Psi e^{-\Psi N}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N}=\frac{T\Psi e^{-\Psi N}}{1-TNe^{-\Psi N}}
\]

\end_inset


\end_layout

\begin_layout Standard
Whereas if we plug in 
\begin_inset Formula $A_{l}=1,A_{h}=0$
\end_inset

 up above, we get
\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N_{l}}=\frac{T\cdot\left[e^{-\Psi N_{l}}\left[\frac{N_{l}}{N_{l}}+N_{l}\Psi-1\right]+0\right]}{N_{l}-\left[Te^{-\Psi N_{l}}N_{l}^{2}\right]}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N_{l}}=\frac{TN_{l}\Psi e^{-\Psi N_{l}}}{N_{l}\cdot\left[1-Te^{-\Psi N_{l}}N_{l}\right]}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\Psi}{\partial N_{l}}=\frac{T\Psi e^{-\Psi N_{l}}}{\left[1-Te^{-\Psi N_{l}}N_{l}\right]}
\]

\end_inset


\end_layout

\begin_layout Standard
Okay, nevermind, everything works out.
\end_layout

\begin_layout Standard
â€“
\end_layout

\begin_layout Section*
Taking partial derivatives of U
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U=\frac{\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Partial derivative wrt some 
\begin_inset Formula $N_{j}$
\end_inset

:
\end_layout

\begin_layout Standard
Holding everything else constant, including 
\begin_inset Formula $\left\{ N_{-i}\right\} $
\end_inset

, and treating 
\begin_inset Formula $U$
\end_inset

 as a function of 
\begin_inset Formula $N_{j}$
\end_inset

, 
\end_layout

\begin_layout Standard
I messed up the derivative of the numerator and need to completely redo
 this.
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
The derivatives of the numerator and denominator are :
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
\frac{\partial}{\partial N_{j}}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right] & =\frac{\partial}{\partial N_{j}}\left[A_{j}N_{j}e^{(U-1)TN_{j}}\right]\\
 & =e^{(U-1)TN_{j}}\cdot\frac{\partial}{\partial N_{j}}\left[A_{j}N_{j}\right]+A_{j}N_{j}\cdot\frac{\partial}{\partial N_{j}}\left[e^{(U-1)TN_{j}}\right]\\
 & =A_{j}e^{(U-1)TN_{j}}+A_{j}N_{j}e^{(U-1)TN_{j}}\cdot\left[\frac{\partial U}{\partial N_{j}}TN_{j}+TU-T\right]\\
 & =A_{j}e^{(U-1)TN_{j}}\cdot\left[1+\frac{\partial U}{\partial N_{j}}TN_{j}^{2}+TUN_{j}-TN_{j}\right]
\end{align*}

\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial N_{j}}\left[\sum_{i}A_{i}N_{i}\right]=A_{j}
\]

\end_inset


\end_layout

\begin_layout Plain Layout
(Which agrees with the derivatives found for the numer and denom of 
\begin_inset Formula $Psi$
\end_inset

 up above if you substitute 
\begin_inset Formula $\Psi\equiv(1-U)T$
\end_inset

 and 
\begin_inset Formula $\frac{\partial\Psi}{\partial N_{j}}\equiv-T\frac{\partial U}{\partial N_{j}}$
\end_inset

)
\end_layout

\begin_layout Plain Layout
Combining these using the quotient rule yields
\begin_inset Formula 
\begin{align*}
\frac{\partial U}{\partial N_{j}} & =\frac{A_{j}e^{(U-1)TN_{j}}\cdot\left[1+\frac{\partial U}{\partial N_{j}}TN_{j}^{2}+TUN_{j}-TN_{j}\right]\left[\sum_{i}A_{i}N_{i}\right]-A_{j}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}\\
 & =\frac{A_{j}e^{(U-1)TN_{j}}\cdot\left[\frac{\partial U}{\partial N_{j}}TN_{j}^{2}\right]}{\left[\sum_{i}A_{i}N_{i}\right]}+\frac{A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\left[\sum_{i}A_{i}N_{i}\right]-A_{j}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
\frac{\partial U}{\partial N_{j}}\cdot\left[1-\frac{A_{j}e^{(U-1)TN_{j}}TN_{j}^{2}}{\left[\sum_{i}A_{i}N_{i}\right]}\right] & =\frac{A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\left[\sum_{i}A_{i}N_{i}\right]-A_{j}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}\\
 & =\frac{A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\left[\sum_{i}A_{i}N_{i}\right]-A_{j}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}
\end{align*}

\end_inset


\begin_inset Formula 
\[
\frac{\partial U}{\partial N_{j}}=\frac{\left[\frac{A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\left[\sum_{i}A_{i}N_{i}\right]-A_{j}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}\right]}{\left[1-\frac{A_{j}e^{(U-1)TN_{j}}TN_{j}^{2}}{\left[\sum_{i}A_{i}N_{i}\right]}\right]}
\]

\end_inset


\end_layout

\begin_layout Plain Layout
The denominator is 
\begin_inset Formula $1-\frac{A_{j}e^{(U-1)TN_{j}}TN_{j}^{2}}{\left[\sum_{i}A_{i}N_{i}\right]}=1-T\frac{A_{j}N_{j}^{2}e^{(U-1)TN_{j}}}{\left[\sum_{i}A_{i}N_{i}\right]}>1-T\frac{\sum_{i}A_{i}N_{i}^{2}e^{(U-1)TN_{i}}}{\sum_{i}A_{i}N_{i}}>0$
\end_inset

.
 (See below for proof.)
\end_layout

\begin_layout Plain Layout
So 
\begin_inset Formula $\frac{\partial U}{\partial N_{j}}$
\end_inset

 is positive when the numerator is positive.
\end_layout

\begin_layout Plain Layout
The numerator is 
\begin_inset Formula 
\begin{align*}
numer & =\frac{A_{j}e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\left[\sum_{i}A_{i}N_{i}\right]-A_{j}\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}\\
 & =A_{j}\frac{e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\left[\sum_{i}A_{i}N_{i}\right]-\left[\sum_{i}A_{i}N_{i}e^{(U-1)TN_{i}}\right]}{\left[\sum_{i}A_{i}N_{i}\right]^{2}}\\
\\
\end{align*}

\end_inset


\end_layout

\begin_layout Plain Layout
In the single-type case, 
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
(agrees with above)
\begin_inset Formula 
\begin{align*}
numer & =\frac{Ne^{(U-1)TN}+TUNNe^{(U-1)TN}-TNNe^{(U-1)TN}-Ne^{(U-1)TN}}{N^{2}}\\
 & =TUe^{(U-1)TN}-Te^{(U-1)TN}\\
 & =(U-1)Te^{(U-1)TN}
\end{align*}

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout
In two type case, with 
\begin_inset Formula $A_{j}=A_{k}=\frac{1}{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
numer & =\frac{1}{2}\frac{e^{(U-1)TN_{j}}\cdot\left[1+TUN_{j}-TN_{j}\right]\left[\frac{N_{j}+N_{k}}{2}\right]-\left[\frac{N_{j}e^{(U-1)TN_{j}}+N_{k}e^{(U-1)TN_{k}}}{2}\right]}{\left[\frac{N_{j}+N_{k}}{2}\right]^{2}}\\
 & =\frac{e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]\left[N_{j}+N_{k}\right]-\left[N_{j}e^{(U-1)TN_{j}}+N_{k}e^{(U-1)TN_{k}}\right]}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}\\
 & =\frac{\left[e^{(U-1)TN_{j}}+(U-1)TUN_{j}e^{(U-1)TN_{j}}\right]\left[N_{j}+N_{k}\right]-\left[N_{j}e^{(U-1)TN_{j}}+N_{k}e^{(U-1)TN_{k}}\right]}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}\\
 & =\frac{\xcancel{N_{j}e^{(U-1)TN_{j}}}+(U-1)TN_{j}^{2}e^{(U-1)TN_{j}}+N_{k}e^{(U-1)TN_{j}}+(U-1)TN_{j}N_{k}e^{(U-1)TN_{j}}-\xcancel{N_{j}e^{(U-1)TN_{j}}}-N_{k}e^{(U-1)TN_{k}}}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}\\
 & =\frac{\left[(U-1)TN_{j}^{2}+N_{k}+(U-1)TN_{j}N_{k}\right]e^{(U-1)TN_{j}}-N_{k}e^{(U-1)TN_{k}}}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}\\
 & =\frac{\left[(U-1)TN_{j}^{2}\left[N_{j}+N_{k}\right]+N_{k}\right]e^{(U-1)TN_{j}}-N_{k}e^{(U-1)TN_{k}}}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}\\
 & =\frac{\left[(U-1)TN_{j}^{2}\left[N_{j}+N_{k}\right]\right]e^{(U-1)TN_{j}}+N_{k}\left[e^{(U-1)TN_{j}}-e^{(U-1)TN_{k}}\right]}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}
\end{align*}

\end_inset


\end_layout

\begin_layout Plain Layout
Now, I've probably made some sort of horrible algebra mistake at this point.
 But suppose I haven't.
 Then looking at the above, 
\end_layout

\begin_layout Plain Layout
The first term 
\begin_inset Formula $\left[(U-1)TN_{j}^{2}\left[N_{j}+N_{k}\right]\right]e^{(U-1)TN_{j}}$
\end_inset

 is negative because 
\begin_inset Formula $TN_{j}^{2}\left[N_{j}+N_{k}\right]e^{(U-1)TN_{j}}>0$
\end_inset

 and 
\begin_inset Formula $U-1<0$
\end_inset

.
\end_layout

\begin_layout Plain Layout
The second term 
\begin_inset Formula $N_{k}\left[e^{(U-1)TN_{j}}-e^{(U-1)TN_{k}}\right]$
\end_inset

 could be either positive or negative.
 If 
\begin_inset Formula $N_{j}>N_{k}$
\end_inset

, then 
\begin_inset Formula $N_{k}\left[e^{(U-1)TN_{j}}-e^{(U-1)TN_{k}}\right]>0$
\end_inset

, (because 
\begin_inset Formula $e^{-(1-U)TN}$
\end_inset

 is a strictly decreasing function of 
\begin_inset Formula $N$
\end_inset

), which means the entire numerator is negative.
 
\end_layout

\begin_layout Plain Layout
So we can conclude that in the case with two types of equal population,
 
\series bold
the larger type decreases 
\begin_inset Formula $U$
\end_inset

 and increases 
\begin_inset Formula $\Psi$
\end_inset

 when they increase connectivity
\series default
.
 This makes intuitive sense.
 But when can the small type have the opposite effect?
\end_layout

\begin_layout Plain Layout
Instead, rearrange it like so: 
\begin_inset Formula 
\begin{align*}
numer & =\frac{e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]\left[N_{j}+N_{k}\right]-\left[N_{j}e^{(U-1)TN_{j}}+N_{k}e^{(U-1)TN_{k}}\right]}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}\\
 & =\frac{N_{j}\cdot\left(e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-e^{(U-1)TN_{j}}\right)+N_{k}\cdot\left(e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-e^{(U-1)TN_{k}}\right)}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}\\
 & =\frac{\sum_{i}\left[\left(e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-e^{(U-1)TN_{i}}\right)N_{i}\right]}{\left[N_{j}+N_{k}\right]\left[N_{j}+N_{k}\right]}
\end{align*}

\end_inset


\end_layout

\begin_layout Plain Layout
What can we say? The whole thing is negative when 
\begin_inset Formula $e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]<0$
\end_inset

.
 This happens when 
\begin_inset Formula $N>\frac{1}{(1-U)T}=\frac{1}{\Psi}$
\end_inset

.
 That is, at least in this setup, 
\series bold
an already fatalistic person always decreases 
\begin_inset Formula $U$
\end_inset

 and increases 
\begin_inset Formula $\Psi$
\end_inset

 when they increase connectivity.
 
\end_layout

\begin_layout Plain Layout
(But a lack of fatalism isn't enough to ensure the thing is positive.
 For one, 
\begin_inset Formula $\left(e^{(U-1)TN_{j}}\cdot\left[1+(U-1)TN_{j}\right]-e^{(U-1)TN_{j}}\right)=\left(e^{(U-1)TN_{j}}\cdot\left[(U-1)TN_{j}\right]\right)<0$
\end_inset

.
 So at least one of the terms in the sum will be negative (also proving
 can't get counterintuitive results for single-type-case)
\end_layout

\begin_layout Plain Layout
So to get the counterintutitive result, needs to be that 
\end_layout

\begin_layout Plain Layout
CONTINUE FROM HERE
\end_layout

