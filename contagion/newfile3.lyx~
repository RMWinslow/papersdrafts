\begin_body

\begin_layout Standard
\begin_inset Formula 
\[
RT(1-RT)^{n}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T(1-RT)^{n}+R\frac{d}{dR}T(1-RT)^{n}
\]

\end_inset


\begin_inset Formula 
\[
T(1-RT)^{n}+RTn(1-RT)^{n-1}(-T)
\]

\end_inset


\begin_inset Formula 
\[
T(1-RT)^{n}-RT^{2}n(1-RT)^{n-1}
\]

\end_inset


\begin_inset Formula 
\[
\left[1-RT-RTn\right]T(1-RT)^{n-1}
\]

\end_inset


\end_layout

\begin_layout Standard
Positive iff
\begin_inset Formula 
\[
\left[1-RT-RTn\right]>0
\]

\end_inset


\begin_inset Formula 
\[
1>RT(1+n)
\]

\end_inset


\begin_inset Formula 
\[
\frac{1}{RT}-1>n
\]

\end_inset


\end_layout

\begin_layout Standard
What is the disesase risk at the transition to lower marginal risk wrt transmiss
ion
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n<\frac{1}{RT}-1
\]

\end_inset


\begin_inset Formula 
\[
n+1=\frac{1}{RT}
\]

\end_inset


\begin_inset Formula 
\[
p(n)=R=1-(1-RT)^{n}
\]

\end_inset


\begin_inset Formula 
\[
p(n)=R=1-(1-\frac{1}{1+n})^{n}
\]

\end_inset


\begin_inset Formula 
\[
p(n)=1-(\frac{n}{1+n})^{n}
\]

\end_inset


\begin_inset Formula 
\[
p(n)=R=1-(1-RT)^{\frac{1}{RT}-1}
\]

\end_inset


\end_layout

\begin_layout Standard
-
\end_layout

